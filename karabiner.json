              {
                "from": {
                  "key_code": "backslash",
                  "modifiers": {
                    "mandatory": ["left_shift"],
                    "optional": "any"
                  }
                },
                "to": [
                  {
                    "key_code": "backslash"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "backslash"
                },
                "to": [
                  {
                    "key_code": "backslash",
                    "modifiers": ["left_shift"]
                  }
                ],
                "type": "basic"
              }
            ]
