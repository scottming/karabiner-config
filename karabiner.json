        "rules": [
          {
            "description": "caps_lock -> ctrl/esc(if_alone).",
            "manipulators": [
              {
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_control"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "capslock+q -> cmd+h",
            "manipulators": [
              {
                "from": {
                  "key_code": "q",
                  "modifiers": {
                    "mandatory": ["left_control"],
                    "optional": "any"
                  }
                },
                "to": [
                  {
                    "key_code": "h",
                    "modifiers": ["left_command"]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "switch \\ and |",
